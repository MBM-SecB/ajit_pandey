@model Department


@{
  ViewData["Title"] = @Model.DepartmentName;
}
<h1>Department Details <a href="/department/index">Go Back</a></h1>
<ul class="list-group">

  
  <li class="list-group-item list-group-item-primary">Department Id: @Model.DepartmentCode</li>
  <li class="list-group-item list-group-item-secondary">Department Name: @Model.DepartmentName</li>
  <li class="list-group-item list-group-item-success">Location: @Model.Location</li>
  <li class="list-group-item list-group-item-success">Description: @Model.Description</li>
  <li class="list-group-item list-group-item-success">Created Date: @Model.CreatedDate</li>
 

  
</ul>